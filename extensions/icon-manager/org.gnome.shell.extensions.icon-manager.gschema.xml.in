<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.icon-manager" path="/org/gnome/shell/extensions/icon-manager/">
    <key type="as" name="top-bar">
      <default>[]</default>
      <_summary>Top Bar icons</_summary>
      <_description>
      List of icons that will be removed from top panel. 
      Gnome Shell Icons:  a11y (accessibility icon), display, keyboard, volume, bluetooth, network, battery
      
      All other icon names that you put here, will be moved from tray bar e.g. gnote, tomboy
      </_description>
    </key>
   </schema>
</schemalist>
